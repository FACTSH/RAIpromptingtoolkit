{
  "version": 2,
  "builds": [
    {
      "src": "rai.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(generate|analyze|run_experiment)",
      "dest": "rai.py"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ]
}
